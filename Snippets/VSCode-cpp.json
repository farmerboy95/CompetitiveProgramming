    "Binary Exponentiation": {
        "prefix": "zzBinPowMod",
        "body": [
          "ll binPowMod(ll a, ll b) {",
          "    a %= MOD;",
          "    ll res = 1;",
          "    while (b > 0) {",
          "        if (b & 1LL) res = res * a % MOD;",
          "        a = a * a % MOD;",
          "        b >>= 1LL;",
          "    }",
          "    return res;",
          "}",
          "",
          "ll add(ll a, ll b) {",
          "    return (a + b) % MOD;",
          "}",
          "",
          "ll sub(ll a, ll b) {",
          "    return (a - b + MOD) % MOD;",
          "}",
          "",
          "ll mul(ll a, ll b) {",
          "    return a * b % MOD;",
          "}"
        ],
        "description": "Binary Exponentiation"
    },
    "Extended Euclid Algorithm": {
        "prefix": "zzExtendedEuclid",
